             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="SplashWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ns.GUI.WPF;component/Languages/en_US.xaml" />
                <ResourceDictionary Source="pack://application:,,,/ns.GUI.WPF;component/Accents/Light.xaml" />
                <ResourceDictionary Source="pack://application:,,,/ns.GUI.WPF;component/Styles/Controls.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
